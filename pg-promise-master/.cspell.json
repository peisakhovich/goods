{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/coverage/**",
    "**/node_modules/**",
    "**/jsdoc/templates/*",
    "**/*.{json}",
    ".cspell.json"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_US",
    "npm",
    "misc",
    "filetypes"
  ],
  "ignoreRegExpList": [
    "/`[^`]*`/"
  ],
  "words": [
    "PostgreSQL",
    "Vitaly",
    "Tomilov",
    "savepoint",
    "savepoints",
    "spex",
    "qrec",
    "Millis"
  ],
  "overrides": [
    {
      "filename": "**/*.{ts,js}",
      "ignoreRegExpList": [
        "/@[a-z]+/",
        "/#(end)?region/"
      ],
      "includeRegExpList": [
        "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
        "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
      ]
    }
  ]
}
